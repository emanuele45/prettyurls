<package-info xmlns="http://www.simplemachines.org/xml/package-info">
	<id>el:prettyurls-extras</id>
	<version>0.9</version>
	<name>Pretty URLs Extras</name>
	<type>modification</type>

	<install>
		<readme parsebbc="true">readme-install.bbc</readme>
		<modification>Sources.xml</modification>
		<code>install.php</code>
	</install>
	<uninstall>
		<modification reverse="true">Sources.xml</modification>
		<code>uninstall.php</code>
	</uninstall>

</package-info>
